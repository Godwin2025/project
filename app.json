{
  "expo": {
    "name": "Healthfirst Mobile App",
    "slug": "healthfirst-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.svg",
    "splash": {
      "image": "./assets/splash.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.healthfirst.mobileapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.healthfirst.mobileapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A comprehensive healthfirst mobile application with telemedicine, health tracking, and prescription management features.",
    "githubUrl": "https://github.com/yourusername/healthfirst-mobile-app"
  }
}